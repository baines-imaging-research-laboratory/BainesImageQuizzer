<?xml version="1.0" encoding="utf-8"?>
<Session>
	<Page Name="Patient1" Descriptor="PlanningCT">
		<Image Format="NRRD" Type="Volume" ID="Fixed Volume">
			<Destination>Red</Destination>
			<Layer>Background</Layer>
			<Orientation>Axial</Orientation>
			<Path>ImageVolumes\Brain\BrainCT_fixed.nrrd</Path>
		</Image>
		<Image ID="Moving Volume" Format="NRRD" Type="Volume">
			<Destination>Red</Destination>
			<Layer>Foreground</Layer>
			<Orientation>Axial</Orientation>
			<Path>ImageVolumes\Brain\BrainCT_moving.nrrd</Path>
		</Image>
		<Image Format="NRRD" Type="Volume" ID="Fixed Volume">
			<Destination>Yellow</Destination>
			<Layer>Background</Layer>
			<Orientation>Sagittal</Orientation>
			<Path>ImageVolumes\Brain\BrainCT_fixed.nrrd</Path>
		</Image>
		<Image Format="NRRD" Type="Volume" ID="Fixed Volume">
			<Destination>Green</Destination>
			<Layer>Background</Layer>
			<Orientation>Coronal</Orientation>
			<Path>ImageVolumes\Brain\BrainCT_fixed.nrrd</Path>
		</Image>
		<QuestionSet Name="QS-Intro" Descriptor="Introduction to Quiz"  SegmentRequired="N" AllowMultipleResponse="N">
			<Question Type="InfoBox" Descriptor="Introduction">
				<Option>Lots of text describing something</Option>
			</Question>
		</QuestionSet>
		<QuestionSet Name="QS-Intro #2" Descriptor="How to - Instructions"  SegmentRequired="N"  AllowMultipleResponse="N">
			<Question Type="InfoBox" Descriptor="Instructions">
				<Option>This is how to run the quiz : .....</Option>
			</Question>
			<Question Type="IntegerValue" Descriptor="Food Order" Max="5">
				<Option>Enter number of pizzas:</Option>
			</Question>
		</QuestionSet>
	</Page>
	<Page Name="Patient1" Descriptor="Follow-up 1">
		<Image Format="NRRD" Type="Volume" ID="Moving Volume" RotateToAcquisition="Y">
			<Destination>Red</Destination>
			<Layer>Background</Layer>
			<Orientation>Axial</Orientation>
			<Path>ImageVolumes\Brain\BrainCT_moving.nrrd</Path>
		</Image>
		<Image Format="NRRD" Type="Volume" ID="Moving Volume" RotateToAcquisition="Y">
			<Destination>Yellow</Destination>
			<Layer>Background</Layer>
			<Orientation>Sagittal</Orientation>
			<Path>ImageVolumes\Brain\BrainCT_moving.nrrd</Path>
		</Image>
		<Image Format="NRRD" Type="Volume" ID="Moving Volume" RotateToAcquisition="Y">
			<Destination>Green</Destination>
			<Layer>Background</Layer>
			<Orientation>Coronal</Orientation>
			<Path>ImageVolumes\Brain\BrainCT_moving.nrrd</Path>
		</Image>
		<QuestionSet Name="QS-Observation" Descriptor="What is your Assessment"  SegmentRequired="Y"  AllowMultipleResponse="Y">
			<Question Type="Radio" Descriptor="Assessment">
				<Option>Injury</Option>
				<Option>Recurrence</Option>
			</Question>
			<Question Type="Radio" Descriptor="Confidence">
				<Option>Yes</Option>
				<Option>Maybe</Option>
				<Option>No</Option>
			</Question>
			<Question Type="IntegerValue" Descriptor="Tumour Observation" Min="-5" Max="5">
				<Option>Enter number of tumours:</Option>
			</Question>
			<Question Type="FloatValue" Descriptor="Tumour Measurment" Max="50">
				<Option>Enter RECIST of largest tumour:</Option>
			</Question>
			<Question Type="Text" Descriptor="First Impressions">
				<Option>Enter your thoughts : </Option>
			</Question>
		</QuestionSet>
	</Page>
	<Page Name="Patient3" Descriptor="Mixed Displays - First Showing" LinkViews="N">
