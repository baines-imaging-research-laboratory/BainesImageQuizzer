		<QuestionSet Name="QS-Demo" Descriptor="Demo buttons" SegmentRequired="Y"  AllowMultipleResponse="Y">
			<Question Type="CheckBox"  Descriptor="Demo Checks">
				<Option>Check 1</Option>
				<Option>Check 2</Option>
			</Question>
			<Question Type="Radio" Descriptor="Demo Radios">
				<Option>Yes</Option>
				<Option>Maybe</Option>
