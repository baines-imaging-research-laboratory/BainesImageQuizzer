-->
	<Page ID="Patient1" Descriptor="PlanningCT" EnableSegmentEditor="Y" PageGroup='1'>
		<Image Type="Volume" ID="Fixed Volume" LabelMapID="Brain-fixed-contour" SegmentRequired="Y">
			<DefaultDestination>Red</DefaultDestination>
			<Layer>Background</Layer>
			<DefaultOrientation>Axial</DefaultOrientation>
			<Path>ImageVolumes\Brain\BrainCT_fixed.nrrd</Path>
		</Image>
		<Image ID="Moving" Type="Volume" LabelMapID="Brain-moving-contour">
			<DefaultDestination>Red</DefaultDestination>
			<Layer>Foreground</Layer>
			<DefaultOrientation>Axial</DefaultOrientation>
			<Path>ImageVolumes\Brain\BrainCT_moving.nii</Path>
		</Image>
		<Image Type="Volume" ID="Fixed Volume" SegmentRequired="Y" ContourVisibility='Fill'>
			<DefaultDestination>Yellow</DefaultDestination>
			<Layer>Background</Layer>
			<DefaultOrientation>Sagittal</DefaultOrientation>
			<Path>ImageVolumes\Brain\BrainCT_fixed.nrrd</Path>
		</Image>
		<Image Type="Volume" ID="Fixed Volume" SegmentRequired="Y">
			<DefaultDestination>Green</DefaultDestination>
			<Layer>Background</Layer>
			<DefaultOrientation>Coronal</DefaultOrientation>
			<Path>ImageVolumes\Brain\BrainCT_fixed.nrrd</Path>
		</Image>
		<QuestionSet ID="QS-Intro" Descriptor="Introduction to Quiz">
			<Question Type="CheckBox" Descriptor="Test Items:">
				<Option>Segment Editor off, Don't allow multiple Response</Option> 
				<Option>NRRD and NIFTI, background and foreground on Red</Option>
			</Question>
		</QuestionSet>
	</Page>
	<Page ID="Patient1" Descriptor="Follow-up 1" AllowMultipleResponse="Y" EnableSegmentEditor="Y" PageGroup='2'>
		<Image Type="Volume" ID="Moving Volume" RotateToAcquisition="Y" LabelMapID="Brain-moving-contour" DisplayLabelMapID="Brain-moving-contour" SegmentRequired="Y">
